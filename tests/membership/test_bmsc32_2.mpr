msc Page_1;
inst PC;
inst Router;
inst Server;
PC: instance;
label e0;
out c,0 to Router;
time e1 [0,1);
endinstance;
Router: instance;
label e1;
in c,0 from PC;
label e5;
out b,3 to Server;
time e6 [5,6);
label e7;
out a,4 to Server;
time e5 [-2,-1];
endinstance;
Server: instance;
label e9;
in a,4 from Router;
time e7 [0,1);
label e6;
in b,3 from Router;
endinstance;
endmsc;
