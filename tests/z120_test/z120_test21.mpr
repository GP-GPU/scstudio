/****************************************************
Example of HMSC with references to bMSC. Final node is not defined at the end of HMSC definition.
****************************************************/

mscdocument Vkres1;

msc Strnka1;
initial connect L0;
L0: reference bmsc1 connect L1;
L2: final;
L1: reference bmsc2 connect L0, L2;
endmsc;

msc bmsc1;
inst A;
inst B;
A: instance;
out yes,0 to B;
endinstance;
B: instance;
in yes,0 from A;
endinstance;
endmsc;

msc bmsc2;
inst A;
inst B;
A: instance;
in no,0 from B;
endinstance;
B: instance;
out no,0 to A;
endinstance;
endmsc;

msc bmsc3;
inst A;
inst B;
A: instance;
in no,0 from B;
out ok,1 to B;
endinstance;
B: instance;
out no,0 to A;
in ok,1 from A;
endinstance;
endmsc;
